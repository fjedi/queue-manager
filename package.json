{
  "name": "@fjedi/queue-manager",
  "version": "6.0.1",
  "description": "Wrapper around Bull queue-manager",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/queue-manager.git"
  },
  "keywords": [
    "fjedi"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/queue-manager/issues"
  },
  "homepage": "https://github.com/fjedi/queue-manager#readme",
  "dependencies": {
    "@fjedi/logger": "1.46.3",
    "@types/bluebird": "3.5.42",
    "@types/bull": "3.15.9",
    "@types/lodash": "4.17.16",
    "bluebird": "3.7.2",
    "bull": "4.11.4",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/expect": "24.3.2",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.100",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "9.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "ts-jest": "29.3.2",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
